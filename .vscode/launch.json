{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "app.main",
      "type": "python",
      "request": "launch",
      "module": "app.main",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["--debug"]
    },
    {
      "name": "app.main backup --source /path",
      "type": "python",
      "request": "launch",
      "module": "app.main",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["--debug", "backup", "--source", "/path"]
    },
    {
      "name": "File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
